{
  "name": "electro-shop-mern",
  "version": "1.0.0",
  "description": "MERN Simple Ecommerce",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server/server",
    "server": "nodemon server/server",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node server/seeder",
    "data:destroy": "node server/seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shahnewaztameem/firehouse-pizza-shop-mern.git"
  },
  "author": "Shahnewaz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shahnewaztameem/firehouse-pizza-shop-mern/issues"
  },
  "homepage": "https://github.com/shahnewaztameem/firehouse-pizza-shop-mern#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "concurrently": "^6.0.1",
    "nodemon": "^2.0.7"
  }
}
